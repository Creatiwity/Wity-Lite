{
	"baseUrl": "{$wity_base_url}libraries",
	"urlArgs": "v={$wity_version}",
	"paths": {
		"apps": "witycms/apps.plugin",
		"themes": "witycms/themes.plugin",
		"jquery": "jquery-1.11.2/jquery-1.11.2.min",
		"tether": "tether-1.3.1/js/tether.min",
		"bootstrap": "bootstrap-4.0.0-alpha.3/js/bootstrap.min",
		"bootstrap-datepicker": "bootstrap-datepicker-1.6.1/js/bootstrap-datepicker.min",
		"wityslider": "wityslider-1.2.9/wityslider"
	},
	"shim": {
		"bootstrap": ["jquery", "tether"],
		"bootstrap-datepicker": ["bootstrap"]
	},
	"waitSeconds": 15
}
